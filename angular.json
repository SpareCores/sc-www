{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sc-www": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@ngx-env/builder:application",
          "options": {
            "outputPath": "dist/sc-www",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js", "src/polyfills.ts"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/sitemap.xml",
              "src/robots.txt",
              "src/manifest.webmanifest",
              "src/_headers",
              "src/email-templates"
            ],
            "styles": [
              "src/styles.scss",
              "./node_modules/ngx-lightbox/lightbox.css",
              "node_modules/survey-core/defaultV2.min.css",
              "node_modules/prismjs/themes/prism-tomorrow.min.css",
              "node_modules/prismjs/plugins/line-highlight/prism-line-highlight.min.css",
              "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.min.css",
              "node_modules/prismjs/plugins/command-line/prism-command-line.min.css",
              "node_modules/prismjs/plugins/toolbar/prism-toolbar.min.css",
              "src/assets/prism-js-override.css"
            ],
            "scripts": [
              "node_modules/flowbite/dist/flowbite.min.js",
              "node_modules/@preline/combobox/index.js"
            ],
            "allowedCommonJsDependencies": [
              "survey-core",
              "survey-angular-ui",
              "crypto-js",
              "nodemailer",
              "handlebars",
              "prismjs",
              "file-saver",
              "@preline/combobox"
            ],
            "server": "src/main.server.ts",
            "prerender": {
              "discoverRoutes": false,
              "routesFile": "prerender_routes.txt"
            },
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "100mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "400kb"
                }
              ],
              "outputHashing": "all",
              "sourceMap": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "ssr": {
                "entry": "server.ts"
              }
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@ngx-env/builder:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "sc-www:build:production"
            },
            "development": {
              "buildTarget": "sc-www:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@ngx-env/builder:extract-i18n",
          "options": {
            "buildTarget": "sc-www:build"
          }
        },
        "test": {
          "builder": "@ngx-env/builder:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics"]
  }
}
